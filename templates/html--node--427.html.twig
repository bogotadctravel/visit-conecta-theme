<!DOCTYPE html>
<html{{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token }}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="{{ base_path ~ directory }}/favicon.ico" type="image/x-icon">
    <link rel="shortcut icon" href="{{ base_path ~ directory }}/favicon.ico" type="image/x-icon">
    <title>{{ head_title|safe_join(' | ') }}</title>
     {{ attach_library('idt_theme/global-styling') }}
    <css-placeholder token="{{ placeholder_token }}">
    <js-placeholder token="{{ placeholder_token }}">
  </head>

  <body {{attributes}}>
    {% include "@idt_theme/includes/header_conecta.html.twig" %}
    <main id="main-content" class="site-content">
      {{ page.content }}
    </main>
    {% include "@idt_theme/includes/footer_conecta.html.twig" %}
    <js-bottom-placeholder token="{{ placeholder_token }}">
  </body>
</html>
